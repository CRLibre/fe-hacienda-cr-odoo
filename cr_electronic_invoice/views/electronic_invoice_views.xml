					<field name="name"/>
					<field name="active"/>
				</tree>
			</field>
		</record>

		<record id="view_resolution_form" model="ir.ui.view">
			<field name="name">resolution.form</field>
			<field name="model">resolution</field>
			<field name="arch" type="xml">
				<form string="Documentos de referencia">
